      \strng{namehash}{3c33895352d51450b1acc6b68f1027c4}
      \strng{fullhash}{9d97f11019d09cb0b89ff3b95be30f3b}
      \strng{fullhashraw}{9d97f11019d09cb0b89ff3b95be30f3b}
      \strng{bibnamehash}{3c33895352d51450b1acc6b68f1027c4}
      \strng{authorbibnamehash}{3c33895352d51450b1acc6b68f1027c4}
      \strng{authornamehash}{3c33895352d51450b1acc6b68f1027c4}
      \strng{authorfullhash}{9d97f11019d09cb0b89ff3b95be30f3b}
