      \strng{namehash}{8ce64ccf76409005ece85d3aa1b76730}
      \strng{fullhash}{3eccd5ad226b9ab1ecb8319d1335b497}
      \strng{fullhashraw}{3eccd5ad226b9ab1ecb8319d1335b497}
      \strng{bibnamehash}{3eccd5ad226b9ab1ecb8319d1335b497}
