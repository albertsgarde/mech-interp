      \strng{authornamehash}{8ce64ccf76409005ece85d3aa1b76730}
      \strng{authorfullhash}{3eccd5ad226b9ab1ecb8319d1335b497}
      \strng{authorfullhashraw}{3eccd5ad226b9ab1ecb8319d1335b497}
