    \entry{tamkin_codebook_2023}{misc}{}{}
      \name{author}{3}{}{%
        {{un=0,uniquepart=base,hash=1209f7e3599bd390bab72d9cca887763}{%
           family={Tamkin},
           familyi={T\bibinitperiod},
